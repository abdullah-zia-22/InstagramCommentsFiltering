{
  "name": "Comments Filtering Instagram",
  "description": "Hide Offensive Comments on Instagram.com",
  "version": "2.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./jquery.js",
      "./background.js",
      "./popup.js"
    ]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "permissions": [
  
    "activeTab",
    "https://www.youtube.com/",
    "https://fyp-nlp.herokuapp.com/comments_prediction",
    "https://www.instagram.com/"
  ]
}
